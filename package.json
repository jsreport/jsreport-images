{
  "name": "jsreport-images",
  "version": "1.0.3",
  "description": "jsreport extension for storing static images inside the store and embedding them into templates",
  "scripts": {
    "test": "mocha test --timeout 10000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-images",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-images.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "license": "LGPL",
  "dependencies": {
    "async-replace": "1.0.1",
    "bluebird": "3.4.7",
    "in-publish": "2.0.0",
    "multer": "1.1.0",
    "shortid": "2.2.6",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.11.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-core": "1.0.1",
    "jsreport-express": "1.0.2",
    "jsreport-studio-dev": "1.0.0",
    "jsreport-templates": "1.0.1",
    "mocha": "2.5.3",
    "standard": "7.1.2",
    "supertest": "1.2.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
